{
  "name": "level-wrangler",
  "version": "2.2.0-wip",
  "description": "Defines and stores json models by indices within a leveldb",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run build && tape ./spec/*.js",
    "build": "rm -rf dist && babel lib --out-dir dist",
    "watch": "babel --watch lib --out-dir dist",
    "prepublish": "npm run build"
  },
  "keywords": [
    "level",
    "leveldb",
    "json",
    "index",
    "indices",
    "schema",
    "model",
    "orm"
  ],
  "author": "Matt Styles @personalurban",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstyles/wrangler"
  },
  "license": "ISC",
  "dependencies": {
    "eventemitter3": "^0.1.6",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "babel": "^4.7.10",
    "level": "^0.18.0",
    "level-party": "^1.0.1",
    "level-sublevel": "^6.4.6",
    "tape": "^3.5.0"
  }
}
